<div id="nav" role="navigation" tabindex="-1">
        <ul>
            {% for page in pages(meta.menu) if page.title and not page.hidden %}
                <a href="{{ page.url }}">
                <li{% if page.id==current_page.id %} {% endif %}>
                {{ page.title }}
                </li>
                </a>
            {% endfor %}
        </ul>
</div>
